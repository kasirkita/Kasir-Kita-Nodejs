    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.2.2",
    "mongoose": "^5.1.3",
    "mongoose-reverse-populate": "^1.1.0",
    "node-storage": "^0.0.9",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "underscore": "^1.10.2",
    "uuid-token-generator": "^1.0.0",
    "validator": "^10.2.0"
  },
